<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="store.tacomall.common.db.mapper.member.MemberMapper">

    <resultMap id="member" type="store.tacomall.common.db.entity.member.Member">
        <id property="id" column="id"></id>
        <association property="memberStatisticsInfo" column="id" select="store.tacomall.common.db.mapper.member.MemberStatisticsInfoMapper.getMemberStatisticsInfoByMemberId"></association>
    </resultMap>


    <select id="getMember" resultMap="member">
        SELECT *
        FROM member
        ${ew.customSqlSegment}
    </select>

</mapper>